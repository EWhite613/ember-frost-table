{{! Template for the frost-table-cell component }}
{{#if cellRenderer}}
  {{! When we rendered our row, it already bound our row and column indices as parameters for the callback handler. }}
  {{component cellRenderer
    hook=(concat hookPrefix '-renderer')
    hookQualifiers=hookQualifiers
    item=item
    value=value

    onCallback=onCallback
  }}
{{else}}
  {{value}}
{{/if}}